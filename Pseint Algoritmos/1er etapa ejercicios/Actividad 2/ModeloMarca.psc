Algoritmo ModeloMarca
	Definir modelo Como Entero;
	Definir marca Como Caracter;
	Escribir "Por favor, inserte la marca del vehiculo: ";
	leer marca;
	Escribir "Por favor, inserte el modelo del vehiculo: ";
	leer modelo;
	Escribir "El modelo es: ", modelo, " la marca es: ", marca;	
FinAlgoritmo
