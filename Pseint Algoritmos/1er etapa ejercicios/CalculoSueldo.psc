Algoritmo CalculoSueldo
	
	Definir precioHora, sueldo, cantidadHoras Como Real;
	
	Escribir "Ingrese las horas trabajadas del mes: ";
	Leer cantidadHoras;
	
	Escribir "Ingrese el precio por hora del trabajador: ";
	Leer precioHora;
	
	sueldo<-precioHora*cantidadHoras;
	
	Escribir "El trabajador debe cobrar: ",sueldo;
	
FinAlgoritmo
