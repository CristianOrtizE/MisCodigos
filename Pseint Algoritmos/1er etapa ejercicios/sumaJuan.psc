Algoritmo sumaJuan
	Definir Numero1, Numero2, resultado como numerico;
	Escribir "Ingrese el primer valor a sumar:";
	Leer Numero1;
	Escribir "Ingrese el segundo valor a sumar:";
	Leer Numero2;
	resultado<-Numero1+Numero2;
	Escribir "El resultado de la suma es: " resultado;
	
FinAlgoritmo
